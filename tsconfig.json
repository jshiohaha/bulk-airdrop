{
    "compilerOptions": {
      "target": "ES2015",
      "module": "CommonJS",
      "strict": true,
      "strictFunctionTypes": true,
      "noImplicitOverride": true,
      "esModuleInterop": true,
      "noUncheckedIndexedAccess": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "lib": ["dom", "es2015"]
    },
  }
  